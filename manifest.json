{
  "name": "Mindfulness",
  "description": "Track your mindfulness journey",
  "version": "1.01",
  "manifest_version": 3,
   "content_scripts": [
    {
        "matches": ["https://www.amazon.com/checkout*", "https://www.ebay.com/*"],
        "js": ["chart.js", "Dashboard.js"]
    }
    ],

    "web_accessible_resources": [
    {
        "resources": ["https://cdn.jsdelivr.net/npm/chart.js", "chart.js", "Dashboard.js"],
        "matches": ["https://www.amazon.com/*", "https://www.ebay.com/*"]
    }
],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  }
}